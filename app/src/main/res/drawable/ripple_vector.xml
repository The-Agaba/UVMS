<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <!-- Ring 1 -->
    <group
        android:name="ring1"
        android:pivotX="50"
        android:pivotY="50"
        android:scaleX="0"
        android:scaleY="0"
